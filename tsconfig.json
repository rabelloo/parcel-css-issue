{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2019",
    "esModuleInterop": true,
    "sourceMap": true,
    "lib": ["esnext", "dom"],

    "alwaysStrict": true,
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": false,
    "strict": true
  }
}
